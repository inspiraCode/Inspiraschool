	<queryString>
		<![CDATA[SELECT cg.id_group, cga.id_assignment, cst.student_name, ca.assignment_name, cts.partial_two, cst.enroll_number,ca.sie_index
FROM cat_group cg
inner join cross_group_assignment cga
on cg.id_group = cga.id_group
inner join cat_assignment ca 
on ca.id_assignment = cga.id_assignment
inner join cross_student_group_assignment csga
on cga.id = csga.id_group_assignment
inner join cat_student cst 
on csga.id_student = cst.Id_student
inner join ctrl_score cts
on cga.id = cts.id_group_assignment AND cst.Id_student = cts.id_student]]>
	</queryString>
